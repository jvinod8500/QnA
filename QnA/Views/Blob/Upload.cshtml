
@{
    ViewBag.Title = "Blob";
    Layout = "~/Views/Shared/_BlobLayout.cshtml";
}
<div class="col-lg-8 col-lg-offset-2 col-md-8 col-sm-8 col-md-offset-2 col-sm-offset-2" style="padding-top:2%">


    @using (Html.BeginForm())
    {
        <div class="form-group">
            <label class="form-inline">Select Account :</label>

            <select class="form-inline" type="" id="selectaccount">
                <option>Asra Account</option>
                <option selected>RaviKoturwar Account</option>
                <option>Joseph Account</option>
            </select>
        </div>
        <div class="form-group">

            <label for="selectFile">Select File</label>
            <input class="" type="file" id="selectFile" />
        </div>

        <div class="form-group">
            <input type="button" id="fileUpload" name="fileUpload" value="Upload" class="btn" style="color:white;background-color:orangered" />
        </div>

        <div class="form-group">
            <div id="progressBar" style="background-color:grey;color:black"></div>
        </div>

        <div class="form-group">
            <label id="statusMessage" style="color:black;font-size: 25px;"></label>
        </div>
    }
</div>